{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/linux-debug/bench/planc_bench",
            "args": ["-i", "${workspaceFolder}/out/build/linux-debug/bench/frontal_10k.h5.mtx", "-j", "${workspaceFolder}/out/build/linux-debug/bench/frontal_10k.h5.dense.mtx"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
                {
                    "description": "source arma-prettyprint",
                    "text": "source /home/andrew/gdb_armadillo_helpers/gdb_helpers/gdb_armadillo_printers.py"
                },
                                {
                    "description": "source stdcomplex-prettyprint",
                    "text": "source /home/andrew/gdb_armadillo_helpers/gdb_helpers/gdb_std_complex_printer.py"
                }
            ]
        }

    ]
}
